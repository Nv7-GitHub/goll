package main

import "external"

func add(a, b int) int {
  return a + b
}

func main() {
  external.Printf("%s\n", "Hello, World!")
  
  a := 1
  b := 2
  c := add(a, b)
  external.Printf("%d + %d is %d\n", a, b, c)

  a := 0.1
  b := 0.2
  c := a + b
  external.Printf("%f + %f is %f\n", a, b, c)

  a := "Hello"
  mid := ", "
  b := "World!"
  c := a + mid + b
  external.Printf("\"%s\" + \"%s\" is \"%s\"\n", a, b, c)

  a := "Hello"
  a += ", "
  a += "World! "
  external.Printf("\"%s\" (Done with +=)\n", a)
}